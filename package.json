{
  "name": "wdio-electron-service",
  "version": "3.5.1",
  "description": "WebdriverIO service to enable Electron testing",
  "author": "Sam Maister <goosewobbler@protonmail.com>",
  "homepage": "https://github.com/webdriverio-community/wdio-electron-service",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "init": "npm install -g pnpm && pnpm i",
    "prepare": "husky install",
    "ci": "pnpm build && pnpm test && pnpm lint",
    "clean": "rm -rf ./node_modules pnpm-lock.yaml ./dist",
    "clean:dist": "rm -rf ./dist",
    "build": "tsc -b .",
    "lint": "eslint --ignore-path .eslintignore \"**/*.{j,t}s\" && prettier --check \"**/*.{j,t}s\"",
    "lint:fix": "eslint --ignore-path .eslintignore \"**/*.{j,t}s\" --fix",
    "format": "prettier --write \"**/*.{j,t}s\"",
    "release": "release-it --github.release",
    "release:ci": "pnpm release --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "pnpm release patch",
    "release:minor": "pnpm release minor",
    "release:major": "pnpm release major",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch-coverage": "jest --watch --coverage",
    "test": "pnpm test:coverage"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-service",
    "electron",
    "chromedriver",
    "tests"
  ],
  "peerDependencies": {
    "electron": "*",
    "electron-chromedriver": "*"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    },
    "electron-chromedriver": {
      "optional": true
    }
  },
  "dependencies": {
    "@wdio/logger": "^7.19.0",
    "ci-info": "^3.3.2",
    "wdio-chromedriver-service": "^8.1.1",
    "webdriverio": "^7.20.4"
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@wdio/cli": "^7.20.4",
    "@wdio/types": "^7.20.3",
    "electron": "^19.0.6",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-wdio": "^7.19.4",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "release-it": "^15.1.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "files": [
    "bin/*",
    "dist/*",
    "main.js",
    "preload.js"
  ],
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix"
    ],
    "**/*.{yml,json,md,ts,js}": [
      "prettier --write"
    ]
  }
}
